{
  "name": "Test",
  "version": "1.1",
  "description": "Description",
  "background": {
    "service_worker": 
      "background.js"
    
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "permissions": [
    "notifications",
    "fontSettings",
    "activeTab",
    "tabs",
    "webRequest",
    "<all_urls>"
  ],
  "options_page": "options.html",
  "manifest_version": 3
}